{
  "manifest_version": 3,
  "name": "Chrome Usage Tracker",
  "author": "miguelms.es",
  "version": "1.1",
  "permissions": [
    "history"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon-transparent.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'; style-src 'self';"
  }
}